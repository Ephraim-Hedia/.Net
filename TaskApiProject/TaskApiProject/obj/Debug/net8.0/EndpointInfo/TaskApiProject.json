      "bearer": {
        "type": "apiKey",
        "description": "JWT Authorization header using the bearer scheme.",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "bearer": [
        "bearer"
      ]
    }
